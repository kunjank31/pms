<%# header links part %>
    <% include head_links %>
        <%# navbar part %>
            <% include navbar %>
                <div class="container mt-5  ">
                    <div class="table-responsive">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">Password Category Name</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% if(records.length>0) { %>
                                    <% records.forEach((data)=> { %>
                                        <tr>
                                            <td>
                                                <%= data.categoryName %>
                                            </td>
                                            <td>
                                                <a href="/passwordCategory/edit/<%= data._id %> "
                                                    class="btn-sm btn-primary"><i class="fas fa-edit"></i></a>
                                                <a href="/passwordCategory/delete/<%= data._id %> "
                                                    onclick="return confirm('Are you sure?')"
                                                    class="btn-sm btn-danger"><i class="fas fa-trash"></i></a>
                                            </td>
                                        </tr>
                                        <% }) %>
                                            <%}%>
                            </tbody>
                        </table>
                    </div>

                </div>
                <%# Footer part %>
                    <% include footer %>